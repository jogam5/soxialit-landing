<div class="row">
	<% if current_user.id == 20 || current_user.id == 4 || current_user.id == 11 || current_user.id == 65 || current_user.id == 27 || current_user.id == 47 || current_user.id == 16 || current_user.id == 265 || current_user.id == 67  %>
		<%= render 'form' %>
	<% else %>
		<%= render 'form_simple' %>
	<% end %>
</div>
<script type="text/javascript">
jQuery(function() {
  return $('#new_painting').fileupload({
    dataType: "script",
    add: function(e, data) {
      var file, types;
      types = /(\.|\/)(gif|jpe?g|png)$/i;
      file = data.files[0];
      if (types.test(file.type) || types.test(file.name)) {
        data.context = $(tmpl("template-upload", file));
        $('#new_painting').append(data.context);
        return data.submit();
      } else {
        return alert("" + file.name + " is not a gif, jpeg, or png image file");
      }
    },
    progress: function(e, data) {
      var progress;
      if (data.context) {
        progress = parseInt(data.loaded / data.total * 100, 10);
        return data.context.find('.bar').css('width', progress + '%');
      }
    }
  });
});

</script>
