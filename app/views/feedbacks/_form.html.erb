<%= form_for(@feedback) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

  <div class="field">
    <%= f.label :content,  "Feedback", :class=> "title_feed" %><br />
    <div class="small_feed">Necesitamos de tu ayuda para mejorar <strong>Soxialit</strong>. Let it go:</div>
	
    <%= f.text_area :content, rows: "10", :class => "span10" %>
  </div>

  <div style="float:right; margin-right:5%" class="actions">
    <%= f.submit "Enviar", :class => "button-want" %>
  </div>
<% end %>