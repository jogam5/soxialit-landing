$(document).ready(function(){
	$('#products').live({
		mouseenter: 
		function(){
			$('.reputation_'+ $(this).data('id')).fadeIn();
			$('.votacion_'+ $(this).data('id')).fadeIn();

			},
		mouseleave:
			function (){
				$('.reputation_'+ $(this).data('id')).hide();
				$('.votacion_'+ $(this).data('id')).hide();

			}
	  });


$(function(){
    $('#profile_nav_bar a').click(function(){
        $('#profile_nav_bar .active').removeClass('active'); // remove the class from the currently selected
        $(this).addClass('active'); // add the class to the newly clicked link
    });
});