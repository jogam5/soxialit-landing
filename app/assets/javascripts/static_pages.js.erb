$(document).ready(function(){
  $('.image_hover').live({
		mouseenter: 
		function(e){
			e.preventDefault();
			$(this).find('.votacion').fadeIn();
			},
		mouseleave:
		function (e){
			e.preventDefault();
			$(this).find('.votacion').fadeOut();
		}
	});
 
	$(function() {
	  $(".new_comment").bind("keypress", function(e) {
	        if (e.keyCode == 13) return false;
	  });
	});		   
	
	$(function(){
		$('#link_about').live({
			mouseenter: 
			function(e){
				e.preventDefault();
				$(this).removeClass('link_about_li').addClass('link_about_border');
				},
			mouseleave:
			function (e){
				e.preventDefault(); 
				$(this).removeClass('link_about_border').addClass('link_about_li');
			}
		});
	});

	
	$('#link_about').find('a').live("click", function(){
		$('#menu_about li').css('background-color', 'white');
		$('#menu_about li').find('a').css('color', '#999');
		$(this).closest('li').css('background-color', "#e6e6e6");
		$(this).closest('li').css('color', '#DD390D');
	});
		
});